# Production Environment Variables

# Application Settings
VITE_APP_NAME=Organism Simulation
VITE_APP_URL=https://organisms.andernet.dev
VITE_APP_VERSION=1.0.0
VITE_APP_ENVIRONMENT=production

# Feature Flags
VITE_ENABLE_DEBUG_MODE=false
VITE_ENABLE_MEMORY_PANEL=false
VITE_ENABLE_PERFORMANCE_MONITORING=false
VITE_ENABLE_DEV_TOOLS=false
VITE_ENABLE_HOT_RELOAD=false

# Simulation Settings
VITE_DEFAULT_ORGANISM_COUNT=25
VITE_MAX_ORGANISM_COUNT=500
VITE_TARGET_FPS=60

# Logging
VITE_LOG_LEVEL=error
VITE_ENABLE_CONSOLE_LOGS=false

# Performance
VITE_ENABLE_PERFORMANCE_PROFILING=false
VITE_MEMORY_LIMIT_MB=200

# UI Settings
VITE_UI_THEME=light
VITE_SHOW_ADVANCED_CONTROLS=false

# Analytics (Production)
VITE_ENABLE_ANALYTICS=true
VITE_ANALYTICS_ENDPOINT=production-analytics

# Error Reporting
VITE_ENABLE_ERROR_REPORTING=true
VITE_ERROR_REPORTING_ENDPOINT=production-errors
