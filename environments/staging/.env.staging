# Staging Environment Variables

# Application Settings
VITE_APP_NAME=Organism Simulation (Staging)
VITE_APP_URL=https://organisms-staging.andernet.dev
VITE_APP_VERSION=0.0.0
VITE_APP_ENVIRONMENT=staging

# Feature Flags
VITE_ENABLE_DEBUG_MODE=false
VITE_ENABLE_MEMORY_PANEL=false
VITE_ENABLE_PERFORMANCE_MONITORING=true
VITE_ENABLE_DEV_TOOLS=false
VITE_ENABLE_HOT_RELOAD=false

# Simulation Settings
VITE_DEFAULT_ORGANISM_COUNT=30
VITE_MAX_ORGANISM_COUNT=750
VITE_TARGET_FPS=60

# Logging
VITE_LOG_LEVEL=warn
VITE_ENABLE_CONSOLE_LOGS=true

# Performance
VITE_ENABLE_PERFORMANCE_PROFILING=true
VITE_MEMORY_LIMIT_MB=300

# UI Settings
VITE_UI_THEME=light
VITE_SHOW_ADVANCED_CONTROLS=true

# Analytics (Staging)
VITE_ENABLE_ANALYTICS=true
VITE_ANALYTICS_ENDPOINT=staging-analytics
