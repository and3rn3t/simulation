# Development Environment Variables

# Application Settings
VITE_APP_NAME=Organism Simulation
VITE_APP_VERSION=0.0.0
VITE_APP_ENVIRONMENT=development

# Feature Flags
VITE_ENABLE_DEBUG_MODE=true
VITE_ENABLE_MEMORY_PANEL=true
VITE_ENABLE_PERFORMANCE_MONITORING=true
VITE_ENABLE_DEV_TOOLS=true
VITE_ENABLE_HOT_RELOAD=true

# Simulation Settings
VITE_DEFAULT_ORGANISM_COUNT=50
VITE_MAX_ORGANISM_COUNT=1000
VITE_TARGET_FPS=60

# Logging
VITE_LOG_LEVEL=debug
VITE_ENABLE_CONSOLE_LOGS=true

# Performance
VITE_ENABLE_PERFORMANCE_PROFILING=true
VITE_MEMORY_LIMIT_MB=500

# UI Settings
VITE_UI_THEME=dark
VITE_SHOW_ADVANCED_CONTROLS=true

# Development Tools
VITE_ENABLE_VISUAL_DEBUG=true
VITE_ENABLE_GRID_OVERLAY=true
